--soru3 )Sütunların her birine göre diğer sütunları güncelleyecek 5 adet UPDATE işlemi 
UPDATE employee SET name='yeniisim' WHERE id=2 RETURNING * ;
SELECT * FROM employee ;
UPDATE employee SET name= 'hatice' WHERE id=5 RETURNING* ;
UPDATE employee SET birthday=null WHERE id =3 RETURNING* ;
UPDATE employee SET name=NULL WHERE id=1 RETURNING * ;
UPDATE employee SET email=null WHERE name LIKE 'R%' RETURNING * ;
--SORU 4 )Sütunların her birine göre ilgili satırı silecek 5 adet DELETE işlemi yapalım.
DELETE FROM employee WHERE email=null RETURNING * ;
DELETE FROM employee WHERE name='pannier' RETURNING * ;
DELETE FROM employee WHERE id<7 RETURNING * ;
DELETE FROM employee WHERE id=3 RETURNING * ;
DELETE FROM employee WHERE birthday IS NULL RETURNING * ;